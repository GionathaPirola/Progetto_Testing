scenario Scenario_VincitaUtenteBase
load SetteEMezzoNC.asm


exec
	par
		pescaUtente := true
		pescata(PC) := SETTE
		pescata(UTENTE) := SETTE
	endpar
;
step

exec
	par
		pescaUtente := true
		pescata(PC) := RE
		pescata(UTENTE) := RE
	endpar
;
step


check vincitore = NONDEFINITO;
