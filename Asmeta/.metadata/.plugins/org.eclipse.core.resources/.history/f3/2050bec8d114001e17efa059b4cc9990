			esac;
		next(vittoria) :=
			case
				((!(((mano = DUE) & (pescata = CINQUE)) | ((mano = CINQUE) & (pescata = DUE))) & !(((mano = ASSO) & (pescata = TRE)) | ((mano = TRE) & (pescata = ASSO))) & !(((mano = SEI) & (pescata = ASSO)) | ((mano = ASSO) & (pescata = SEI))) & !(((pescata = TRE) & (mano = QUATTRO)) | ((mano = TRE) & (pescata = QUATTRO))) & !((mano = ASSO) & (pescata = ASSO)) & !((mano = DUE) & (pescata = DUE)) & !(((pescata = ASSO) & (mano = SETTE)) | ((mano = ASSO) & (pescata = SETTE))) & !(((pescata = TRE) & (mano = DUE)) | ((mano = TRE) & (pescata = DUE))) & !(((pescata = SEI) & (mano = DUE)) | ((mano = SEI) & (pescata = DUE))) & !((mano = TRE) & (pescata = TRE)) & !(((mano = TRE) & (pescata = CINQUE)) | ((pescata = TRE) & (mano = CINQUE))) & !(((pescata = ASSO) & (mano = DUE)) | ((mano = ASSO) & (pescata = DUE))) & !(((mano = ASSO) & (pescata = CINQUE)) | ((pescata = ASSO) & (mano = CINQUE))) & ((pescata = QUATTRO) & (mano = QUATTRO)) & !(((pescata = ASSO) & (mano = QUATTRO)) | ((mano = ASSO) & (pescata = QUATTRO))) & !(((pescata = DUE) & (mano = QUATTRO)) | ((pescata = QUATTRO) & (mano = DUE)))) | ((!(((mano = DUE) & (pescata = CINQUE)) | ((mano = CINQUE) & (pescata = DUE))) & !(((mano = ASSO) & (pescata = TRE)) | ((mano = TRE) & (pescata = ASSO))) & !(((mano = SEI) & (pescata = ASSO)) | ((mano = ASSO) & (pescata = SEI))) & !(((pescata = TRE) & (mano = QUATTRO)) | ((mano = TRE) & (pescata = QUATTRO))) & !((mano = ASSO) & (pescata = ASSO)) & !((mano = DUE) & (pescata = DUE)) & !(((pescata = ASSO) & (mano = SETTE)) | ((mano = ASSO) & (pescata = SETTE))) & !(((pescata = TRE) & (mano = DUE)) | ((mano = TRE) & (pescata = DUE))) & !(((pescata = SEI) & (mano = DUE)) | ((mano = SEI) & (pescata = DUE))) & !((mano = TRE) & (pescata = TRE)) & (((mano = TRE) & (pescata = CINQUE)) | ((pescata = TRE) & (mano = CINQUE))) & !(((pescata = ASSO) & (mano = DUE)) | ((mano = ASSO) & (pescata = DUE))) & !(((mano = ASSO) & (pescata = CINQUE)) | ((pescata = ASSO) & (mano = CINQUE))) & !(((pescata = ASSO) & (ma