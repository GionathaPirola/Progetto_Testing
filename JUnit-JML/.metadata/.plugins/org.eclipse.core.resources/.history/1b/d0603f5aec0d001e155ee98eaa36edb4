import static org.junit.Assert.*;

import org.junit.Test;

public class testGioco {

	@Test
	public void testIstruzioni() {

		Gioco g = new Gioco(3);
		int[] pescata1 = {1,1,8};
		assertEquals(3,g.giocata(pescata1));
		
		int[] pescata2 = {1,1,8};
		assertEquals(3,g.giocata(pescata2));
	}

}
